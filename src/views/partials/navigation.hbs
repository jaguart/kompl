<script>
  function highlight_here() {
    let $here = window.location.pathname.substring(window.location.pathname.lastIndexOf('/')+1)
    let $anchor = $(`a[href$="${$here}"]`)
    if ( $anchor )  $anchor.addClass('thispage')
  }
  document.addEventListener("DOMContentLoaded", (ev) => {
    highlight_here()
  })
</script>
